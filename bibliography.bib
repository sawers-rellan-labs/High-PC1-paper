@ARTICLE{Bilinski2018-ve,
  title    = "Parallel altitudinal clines reveal trends in adaptive evolution
              of genome size in Zea mays",
  author   = "Bilinski, Paul and Albert, Patrice S and Berg, Jeremy J and
