@article {Wang2020-mp,
	author = {Wang, Li and Josephs, Emily B. and Lee, Kristin M. and Roberts, Lucas M. and Rell{\'a}n-{\'A}lvarez, Rub{\'e}n and Ross-Ibarra, Jeffrey and Hufford, Matthew B.},
	title = {Molecular Parallelism Underlies Convergent Highland Adaptation of Maize Landraces},
